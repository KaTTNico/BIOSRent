<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControladorAlquiler</servlet-name>
        <servlet-class>MVC.presentacion.servlets.controladores.ControladorAlquiler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorCliente</servlet-name>
        <servlet-class>MVC.presentacion.servlets.controladores.ControladorCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorEmpleado</servlet-name>
        <servlet-class>MVC.presentacion.servlets.controladores.ControladorEmpleado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorInicio</servlet-name>
        <servlet-class>MVC.presentacion.servlets.controladores.ControladorInicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorVehiculo</servlet-name>
        <servlet-class>MVC.presentacion.servlets.controladores.ControladorVehiculo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorAlquiler</servlet-name>
        <url-pattern>/ControladorAlquiler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorCliente</servlet-name>
        <url-pattern>/ControladorCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorEmpleado</servlet-name>
        <url-pattern>/ControladorEmpleado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorInicio</servlet-name>
        <url-pattern>/ControladorInicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorVehiculo</servlet-name>
        <url-pattern>/ControladorVehiculo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
  
  
     
</web-app>
